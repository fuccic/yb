  .table--small-hide {
    display: none !important;
  }
  .table__section + .table__section {
    position: relative;
    margin-top: 10px;
    padding-top: 15px;
  }
  .table__section + .table__section:after {
    content: '';
    display: block;
    position: absolute;
    top: 0;
    left: 15px;
    right: 15px;
    border-bottom: 1px solid {{ settings.color_borders }};
  }
}

